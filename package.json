{
  "name": "node-rest-api",
  "version": "1.1.0",
  "description": "simple node restful api template using express,mongoose,mocha",
  "scripts": {
    "clean": "rimraf dist",
    "build": "NODE_ENV=production babel src --out-dir dist",
    "serve": "node dist/main",
    "start:babel": "babel-node src/index",
    "start": "NODE_ENV=develop nodemon --watch src --exec npm run start:babel -L"
  },
  "author": "",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.5.2",
    "babel-eslint": "^6.0.4",
    "babel-plugin-module-resolver": "^2.5.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-env": "^1.2.2",
    "install": "^0.8.1",
    "npm": "^3.10.8",
    "redis": "^2.7.1",
    "rimraf": "^2.5.2"
  },
  "dependencies": {
    "body-parser": "^1.15.0",
    "cors": "^2.7.1",
    "express": "^4.13.4",
    "mongoose": "^4.7.0",
    "nodemon": "^1.11.0",
    "redis": "^2.7.1"
  },
  "license": "MIT"
}
